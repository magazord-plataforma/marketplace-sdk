{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "code": {
            "type": "integer",
            "description": "Código"
        },
        "message": {
            "type": "string",
            "description": "Mensagem"
        },
        "httpCode": {
            "type": "integer",
            "description": "Código HTTP"
        },
        "detail": {
            "type": "array",
            "description": "Detalhes",
            "items": {
                "type": "string"
            }
        }
    },
    "required": ["code", "message", "httpCode"]
}